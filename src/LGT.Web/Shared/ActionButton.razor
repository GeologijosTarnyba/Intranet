@code {
  [Parameter] public RenderFragment ChildContent { get; set; }
  [Parameter] public EventCallback onclick { get; set; }
  [Parameter] public bool ParamIsActive { get; set; } = true;
}
@if (ParamIsActive)
{
  <a href="" @onclick=@this.onclick @onclick:preventDefault>
    @ChildContent
  </a>
}
else
{
  @ChildContent
}