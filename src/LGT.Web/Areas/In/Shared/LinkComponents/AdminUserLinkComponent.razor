@if (this.Href != null)
{
  <LinkComponent ParamHref=@this.Href
    ParamName=@this.Name
    ParamLinkType=@this.ParamLinkType
  >
  </LinkComponent>
}
@code {
  [Parameter] public int? ParamID { get; set; }
  [Parameter] public string? ParamName { get; set; }
  [Parameter] public LinkTypes ParamLinkType { get; set; }
  private string? Href
  {
    get
    {
      if (this.ParamID != null)
      {
        return $"/in/admin/users/{ParamID}";
      }
      return null;
    }
  }
  private string? Name
  {
    get
    {
      if (ParamName != null)
        return this.ParamName;
      return null;
    }
  }
}
