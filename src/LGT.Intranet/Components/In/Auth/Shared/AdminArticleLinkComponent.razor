@if(this.Href != null){
  <LinkComponent ParamHref=@this.Href ParamName=@this.ParamTitle ParamLinkType=@this.ParamLinkType />
}
@code {
  [Parameter] public Guid? ParamID { get; set; }
  [Parameter] public string? ParamTitle { get; set; }
  [Parameter] public LinkTypes ParamLinkType{get; set; }

  private string? Href {
    get {
      if(this.ParamID != null){
        return $"/in/intranet/articles/{ParamID}";
      }
      return null;
    }
  }
}
